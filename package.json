{
  "name": "eleventy_boiler_plate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "del:dist": "rimraf ./dist",
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "ELEVENTY_ENV=production npx eleventy",
    "build": "npm-run-all --parallel dev:assets dev:site",
    "dev": "npm run dev:assets & npm run dev:site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "ELEVENTY_ENV=development npx eleventy --serve",
    "prod": "npm-run-all del:dist build:assets build:site",
    "serve:prod": "serve ./dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "core-js": "^3.11.1",
    "css-loader": "^5.2.4",
    "fibers": "^5.0.0",
    "html-minifier": "^4.0.0",
    "md5-file": "^5.0.0",
    "mini-css-extract-plugin": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.2.0",
    "rimraf": "^2.7.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "netlify-cli": "^3.23.3"
  }
}
